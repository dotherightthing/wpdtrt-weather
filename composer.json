{
    "name": "dotherightthing/wpdtrt-weather",
    "description": "Displays historical weather information for the GPS location determined by the Featured Image.",
    "type": "library",
    "keywords": ["weather", "forecast", "GPS"],
    "license": "ISC",
    "authors": [
        {
            "name": "Dan Smith",
            "email": "dev@dotherightthing.co.nz",
            "homepage": "https://github.com/dotherightthing/wpdtrt-weather",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@dotherightthing.co.nz",
        "issues": "https://github.com/dotherightthing/wpdtrt-weather/issues"
    },
    "require": {
        "dotherightthing/wpdtrt-plugin-boilerplate": "^1.4.39"
    },
    "require-dev": {
        "phpdocumentor/phpdocumentor": "2.9.0",
        "phpunit/phpunit": "^5.7",
        "psy/psysh" : "~0.6",
        "wp-cli/wp-cli" : "~0.22",
        "wp-coding-standards/wpcs": "^0.14.1",
        "DealerDirect/phpcodesniffer-composer-installer": "^0.4.4",
        "wp-cli/wp-cli" : "~0.22",
        "dotherightthing/wpdtrt-exif": "^0.1.12"
    },
    "extras": {
        "require-wp": [
            {
                "name": "DTRT EXIF",
                "host": "github",
                "repository": "dotherightthing/wpdtrt-exif",
                "file": "wpdtrt-exif.php",
                "description": "Retrieves the forecast location from the featured image"
            }
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dotherightthing/wpdtrt-plugin-boilerplate"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dotherightthing/wpdtrt-debug"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dotherightthing/tgm-plugin-activation"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dotherightthing/wpdtrt-exif"
        },
        {
            "type": "vcs",
            "url": "https://github.com/prairiewest/PHPconvertDMSToDecimal"
        }
    ],
    "config": {
        "vendor-dir": "vendor"
    }
}
